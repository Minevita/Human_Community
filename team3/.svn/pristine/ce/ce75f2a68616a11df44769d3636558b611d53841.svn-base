/* 16 */
.join_type .btn-16 {
  border: none;
  color: #000;
}
.join_type .btn-16-1:after {
  position: absolute;
  content: "";
  width: 0;
  height: 100%;
  top: 0;
  left: 0;
  direction: rtl;
  z-index: -1;
  box-shadow:
   -7px -7px 20px 0px #fff9,
   -4px -4px 5px 0px #fff9,
   7px 7px 20px 0px rgb(0 123 255 / 84%), 
   4px 4px 5px 0px #007bff;
  transition: all 0.3s ease;
}

.join_type .btn-16-2:after {
  position: absolute;
  content: "";
  width: 0;
  height: 100%;
  top: 0;
  left: 0;
  direction: rtl;
  z-index: -1;
  box-shadow:
   -7px -7px 20px 0px #fff9,
   -4px -4px 5px 0px #fff9,
   7px 7px 20px 0px #d39e00, 
   4px 4px 5px 0px #ffc107;
  transition: all 0.3s ease;
}

.join_type .btn-16:hover {
  color: #000;
}
.join_type .btn-16:hover:after {
  left: auto;
  right: 0;
  width: 100%;
}
.join_type .btn-16:active {
  top: 2px;
}



