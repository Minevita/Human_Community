/* --------------- header.jsp --------------- */
header {
	width: 100%;
}

header .head {
	width: 1100px;
    height: 120px;
    margin: 0 auto;
    display: flex;
    align-items: center;
}

header .head h1 {
	position: relative;
	float: left;
}

header .wrap_search {
    width: 457px;
    margin-left: 30px;
}

header #dataTable_filter {
    float: left;
    width: 312px;
    background: #fff;
    margin: 4px 0 0 4px;
}

header .top_search {
	clear:both;
    width: 456px;
    height: 46px;
    background: #3b4890;
}

header #keyword {
	background: transparent;
    width: 295px;
    height: 38px;
    padding: 0 9px;
    border: none;
    outline: none;
    color: #333;
    font-size: 14px;
    font-weight: bold;
}


header .head .wrap_search select {
    margin-left: 10px;
    margin-top: 4px;
    height: 38px;
}

header #btn_search img {
    width: 45px;
    height: 45px;
}

header #btn_search:ACTIVE img {
	box-shadow: 1px 1px 0 rgb(0,0,0,0.5); position: relative; top:2px;
}

header .gnb_bar {
    min-width: 1100px;
    height: 45px;
    margin-bottom: 10px;
    background-color: #3b4890;
    border: 1px solid #4876ef;
    border-width: 1px 0 1px 0;
}


header .topMenu {
    width: 100%;
    min-width: 1280px;
	display: flex;
    justify-content: center;
	align-items: center;
	box-sizing: border-box;
    transition: .2s ease-in-out;
	transform: translateY(-5px);
    position: absolute; 
    z-index: 800;
}

header .topMenu::before {
	position: absolute;
	top: 10%;
	left: 0;
	width: 100%;
	color: #fff;
	font: 400 24px/1 'Lato', sans-serif;
	text-align: center;
}

header .topMenu ul {
    text-align: center;
	margin: 0;
	padding: 0;
	list-style: none;
}

header .topMenu ul::after {
	display: block;
	clear: both;
	content: '';
}
header .topMenu ul li {
    width: 200px;
    text-align: center;
    position: relative;
    display: inline-block;
}
header .topMenu ul li:not(:first-child) {
	border-left: none;
}

header .topMenu #list_title {
	font-weight: bolder;
}

header .topMenu ul li:hover > a {
    color: rgb(220, 156, 22);
}
header .topMenu ul li a {
	display: inline-block;
	padding: 1em 3em;
	color: #fff;
	font: 400 18px/1 'Lato', sans-serif;
	text-align: center;
	text-decoration: none;
	white-space: nowrap;
}
header .topMenu ul ul {
	position: absolute;
	top: 100%;
	left: 1px;
}
header .topMenu ul ul li {
	float: none;
	margin: 0;
    margin-left: 1px;
}
header .topMenu ul ul li:not(:first-child) {
	border-top: none;
}
header .topMenu ul ul ul {
	position: absolute;
	top: -2px;
	left: 100%;
}

header .topMenu_control {
    width: 100%;
}

header .topMenu_control>li>a {
    font-weight: bolder;
    font-size: larger;
}

header .topMenu ul ul {
	visibility: hidden;
	opacity: 0;
	transition: .2s ease-in-out;
	transform: translateY(10px);
}
header .topMenu ul ul ul {
	transform: translateX(-20px) translateY(0);
}

header .topMenu:hover {
    background: #3b4890d5;
    padding-bottom: 170px;
    visibility: visible;
	transform: translateY(0);
}

header .topMenu:hover .topMenu_control > li ul {
	visibility: visible;
	opacity: 1;
	transform: translateY(0);
}

header .topMenu ul ul li:hover > ul {
	transform: translateX(0) translateY(0);
}

header .custom-btn {
	width: 130px;
	height: 40px;
	color: #fff;
	border-radius: 5px;
	padding: 10px 25px;
	font-family: 'Lato', sans-serif;
	font-weight: 500;
	background: transparent;
	cursor: pointer;
	transition: all 0.3s ease;
	position: relative;
	display: inline-block;
	/* box-shadow:inset 2px 2px 2px 0px rgba(255,255,255,.5),
	 7px 7px 20px 0px rgba(0,0,0,.1),
	 4px 4px 5px 0px rgba(0,0,0,.1); */
	outline: none;
	
}

/* 3 */
header .btn-3 {
  
  width: 51px;
  height: 35px;
  line-height: 42px;
  padding: 0;
  border: none;
  font-weight: bolder;
  background: rgb(6,14,131);
  border: none;
}
header .btn-3 span {
  position: relative;
  display: block;
  width: 100%;
  height: 100%;
}
header .btn-3:before,
header .btn-3:after {
  position: absolute;
  content: "";
  right: 0;
  top: 0;
   background: snow;
  transition: all 0.3s ease;
}
header .btn-3:before {
  height: 0%;
  width: 2px;
}
header .btn-3:after {
  width: 0%;
  height: 2px;
}
header .btn-3:hover{
  background: rgb(0,172,238);
	background: linear-gradient(0deg, rgba(0,172,238,1) 0%, rgba(2,126,251,1) 100%);
  box-shadow: none;
}
header .btn-3:hover:before {
  height: 100%;
}
header .btn-3:hover:after {
  width: 100%;
}
header .btn-3 span:hover{
   color: snow;
   font-weight: bolder;
}
header .btn-3 span:before,
.btn-3 span:after {
  position: absolute;
  content: "";
  left: 0;
  bottom: 0;
   background: snow;
  transition: all 0.3s ease;
}
header .btn-3 span:before {
  width: 2px;
  height: 0%;
}
header .btn-3 span:after {
  width: 0%;
  height: 2px;
}
header .btn-3 span:hover:before {
  height: 100%;
}
header .btn-3 span:hover:after {
  width: 100%;
}


