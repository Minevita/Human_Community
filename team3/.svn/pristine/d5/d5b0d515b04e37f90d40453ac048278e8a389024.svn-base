.join_type button {
  margin: 20px;
}

.join_type #btn-16-1 {
  width: 454px;
  height: 40px;
  color: #ffffff;
  border-radius: 5px;
  padding: 10px 25px;
  font-family: 'Lato', sans-serif;
  font-weight: 500;
  background: linear-gradient(
	0deg, rgb(0 118 163) 0%, rgb(140 197 255) 100%);
  cursor: pointer;
  transition: all 0.3s ease;
  position: relative;
  display: inline-block;
  box-shadow:inset 2px 2px 2px 0px rgba(255,255,255,.5),
   7px 7px 20px 0px rgba(0,0,0,.1),
   4px 4px 5px 0px #007bffb8;
  outline: none;
}

.join_type #btn-16-2 {
	width: 454px;
  height: 40px;
  color: #ffffff;
  border-radius: 5px;
  padding: 10px 25px;
  font-family: 'Lato', sans-serif;
  font-weight: 500;
  background: #FEE500;
  cursor: pointer;
  transition: all 0.3s ease;
  position: relative;
  display: inline-block;
  box-shadow:
   inset 2px 2px 2px 0px rgb(255 255 255 / 50%), 
   7px 7px 20px 0px rgb(0 0 0 / 10%), 
   4px 4px 5px 0px #ffc107b5;
  outline: none;
}


/* 16 */
.btn-16 {
  border: none;
  color: #000;
}
.btn-16-1:after {
  position: absolute;
  content: "";
  width: 0;
  height: 100%;
  top: 0;
  left: 0;
  direction: rtl;
  z-index: -1;
  box-shadow:
   -7px -7px 20px 0px #fff9,
   -4px -4px 5px 0px #fff9,
   7px 7px 20px 0px rgb(0 123 255 / 84%), 
   4px 4px 5px 0px #007bff;
  transition: all 0.3s ease;
}

.btn-16-2:after {
  position: absolute;
  content: "";
  width: 0;
  height: 100%;
  top: 0;
  left: 0;
  direction: rtl;
  z-index: -1;
  box-shadow:
   -7px -7px 20px 0px #fff9,
   -4px -4px 5px 0px #fff9,
   7px 7px 20px 0px #d39e00, 
   4px 4px 5px 0px #ffc107;
  transition: all 0.3s ease;
}

.btn-16:hover {
  color: #000;
}
.btn-16:hover:after {
  left: auto;
  right: 0;
  width: 100%;
}
.btn-16:active {
  top: 2px;
}



